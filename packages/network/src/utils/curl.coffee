
module.exports =
  # See `man 3 libcurl-errors`
  error: (exit_code) ->
    switch exit_code
      when 1 then 'CURLE_UNSUPPORTED_PROTOCOL'
      when 2 then 'CURLE_FAILED_INIT'
      when 3 then 'CURLE_URL_MALFORMAT'
      when 4 then 'CURLE_NOT_BUILT_IN'
      when 5 then 'CURLE_COULDNT_RESOLVE_PROXY'
      when 6 then 'CURLE_COULDNT_RESOLVE_HOST'
      when 7 then 'CURLE_COULDNT_CONNECT'
      when 8 then 'CURLE_FTP_WEIRD_SERVER_REPLY'
      when 9 then 'CURLE_REMOTE_ACCESS_DENIED'
      when 10 then 'CURLE_FTP_ACCEPT_FAILED'
      when 11 then 'CURLE_FTP_WEIRD_PASS_REPLY'
      when 12 then 'CURLE_FTP_ACCEPT_TIMEOUT'
      when 13 then 'CURLE_FTP_WEIRD_PASV_REPLY'
      when 14 then 'CURLE_FTP_WEIRD_227_FORMAT'
      when 15 then 'CURLE_FTP_CANT_GET_HOST'
      when 17 then 'CURLE_FTP_COULDNT_SET_TYPE'
      when 18 then 'CURLE_PARTIAL_FILE'
      when 19 then 'CURLE_FTP_COULDNT_RETR_FILE'
      when 21 then 'CURLE_QUOTE_ERROR'
      when 22 then 'CURLE_HTTP_RETURNED_ERROR'
      when 23 then 'CURLE_WRITE_ERROR'
      when 25 then 'CURLE_UPLOAD_FAILED'
      when 26 then 'CURLE_READ_ERROR'
      when 27 then 'CURLE_OUT_OF_MEMORY'
      when 28 then 'CURLE_OPERATION_TIMEDOUT'
      when 30 then 'CURLE_FTP_PORT_FAILED'
      when 31 then 'CURLE_FTP_COULDNT_USE_REST'
      when 33 then 'CURLE_RANGE_ERROR'
      when 34 then 'CURLE_HTTP_POST_ERROR'
      when 35 then 'CURLE_SSL_CONNECT_ERROR'
      when 36 then 'CURLE_BAD_DOWNLOAD_RESUME'
      when 37 then 'CURLE_FILE_COULDNT_READ_FILE'
      when 38 then 'CURLE_LDAP_CANNOT_BIND'
      when 39 then 'CURLE_LDAP_SEARCH_FAILED'
      when 41 then 'CURLE_FUNCTION_NOT_FOUND'
      when 42 then 'CURLE_ABORTED_BY_CALLBACK'
      when 43 then 'CURLE_BAD_FUNCTION_ARGUMENT'
      when 45 then 'CURLE_INTERFACE_FAILED'
      when 47 then 'CURLE_TOO_MANY_REDIRECTS'
      when 48 then 'CURLE_UNKNOWN_OPTION'
      when 49 then 'CURLE_TELNET_OPTION_SYNTAX'
      when 51 then 'CURLE_PEER_FAILED_VERIFICATION'
