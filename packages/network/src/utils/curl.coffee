      when 83 then 'CURLE_SSL_ISSUER_ERROR'
      when 84 then 'CURLE_FTP_PRET_FAILED'
      when 85 then 'CURLE_RTSP_CSEQ_ERROR'
      when 86 then 'CURLE_RTSP_SESSION_ERROR'
      when 87 then 'CURLE_FTP_BAD_FILE_LIST'
      when 88 then 'CURLE_CHUNK_FAILED'
      when 89 then 'CURLE_NO_CONNECTION_AVAILABLE'
      else undefined
