      when 56 then 'CURLE_RECV_ERROR'
      when 58 then 'CURLE_SSL_CERTPROBLEM'
      when 59 then 'CURLE_SSL_CIPHER'
      when 60 then 'CURLE_SSL_CACERT'
      when 61 then 'CURLE_BAD_CONTENT_ENCODING'
      when 62 then 'CURLE_LDAP_INVALID_URL'
      when 63 then 'CURLE_FILESIZE_EXCEEDED'
      when 64 then 'CURLE_USE_SSL_FAILED'
      when 65 then 'CURLE_SEND_FAIL_REWIND'
      when 66 then 'CURLE_SSL_ENGINE_INITFAILED'
      when 67 then 'CURLE_LOGIN_DENIED'
      when 68 then 'CURLE_TFTP_NOTFOUND'
      when 69 then 'CURLE_TFTP_PERM'
